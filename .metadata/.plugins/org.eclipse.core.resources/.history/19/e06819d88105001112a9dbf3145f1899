package formatTask;

public class FormatTask02 {
	public static void main(String[] args) {
//      %자릿수d
//      %5d
      
//      국어점수, 영어점수, 수학점수를 받아서 서식에 맞게 출력
//      이름: 이규학
//      수학:    20점
//      국어:    40점
//      영어:    15점
//      총점:    75점
		
		String name = null;
		int math = 0, korean = 0, english = 0, total = 0;
		
		name = "URA";
		math = 99;
		korean = 50;
		english = 60;
		total = math + korean + english;
		
		System.out.printf("이름: %10s\n", name);
		System.out.printf("수학: %10d\n", math);
		System.out.printf("국어: %10d\n", korean);
		System.out.printf("영어: %10d\n", english);
		System.out.printf("총점: %10d\n", total);
	}
}
